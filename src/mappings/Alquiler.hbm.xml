<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="dao">

    <class name="Alquiler" table="alquiler">
    	
    	<id name="idAlquiler" column="idAlquiler"/>
		
		<many-to-one name="actividad" column="idActividad"/>
		<many-to-one name="propiedad" column="idPropiedad"/>
		<many-to-one name="cliente" column="dniCliente"/>
		<property name="fechaInicio" column="fechaInicio" type="date"/>
		<property name="fechaFin" column="fechaFin" type="date"/>
		<property name="precio" column="precio" type="double"/>
			
   </class>
   
</hibernate-mapping>