<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="dao">

    <class name="Alquiler" table="alquiler">
    	
    	<id name="idAlquiler" column="idAlquiler">
    		<generator class="native"/>
    	</id>
		
		<many-to-one name="actividad" class="Actividad">
            <column name="idActividad"/>
        </many-to-one>
        
        <many-to-one name="propiedad" class="Propiedad">
            <column name="idPropiedad"/>
        </many-to-one>
        
        <many-to-one name="cliente" class="Cliente">
            <column name="dniCliente"/>
        </many-to-one>
		
		<property name="fechaInicio" column="fecha_inicio" type="date"/>
		<property name="fechaFin" column="fecha_fin" type="date"/>
		<property name="precio" column="precio" type="double"/>
			
   </class>
   
</hibernate-mapping>