<hibernate-mapping>

    <class name="dao.Propiedad" table="PROPIEDAD">

        <id name="id" column="idPropiedad"/>
        <property name="nombre" column="NOMBRE" not-null="true"/>
		
   </class>
   
</hibernate-mapping>